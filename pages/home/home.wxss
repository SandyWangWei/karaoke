page {
  width: 100%;
  height: 100%;
}

.image-bg {
  height: 100%;
  width: 100%;
}

.page-top {
  display: flex;
  flex-direction: column;
  margin-top: 50rpx;
  align-items: center;
  position: absolute;
  z-index: 1;
}

.image-icon {
  width: 113px;
  height: 151px;
  margin-top: 100px;
}

.home {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100%;
}

.start-button{
  margin-top: 100px;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 30px
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.text-match {
  margin-top: 320px;
  position: absolute;
    color: #fff;
  border-radius: 3%;
}